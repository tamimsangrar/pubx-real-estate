# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# Twilio Configuration (Voice Features)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Google Calendar API (Calendar Booking)
GOOGLE_CALENDAR_CREDENTIALS=your_google_service_account_json
GOOGLE_CALENDAR_ID=your_calendar_id

# ElevenLabs (Voice TTS)
ELEVENLABS_API_KEY=your_elevenlabs_api_key

# Netlify Configuration
NETLIFY_SITE_ID=your_netlify_site_id
NETLIFY_ACCESS_TOKEN=your_netlify_access_token

# Application Configuration
NODE_ENV=production
LOG_LEVEL=info
CORS_ORIGIN=https://your-domain.netlify.app

# Lead Scoring Configuration
LEAD_SCORE_THRESHOLD=70
LEAD_SCORE_CRON_INTERVAL=*/5 * * * *  # Every 5 minutes 